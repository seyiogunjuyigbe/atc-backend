{
  "appName": "APP_NAME",
  "env": "NODE_ENV",
  "host": "HOST",
  "port": "PORT",
  "appUrl": "APP_URL",
  "testDbUrl": "TEST_DATABASE_URL",
  "frontEndUrl": "FRONTEND_URL",
  "enableLogging": "LOG_ERRORS",
  "adminSupportEmail": "ADMIN_SUPPORT_EMAIL",
  "debugEmail": [
    "donriddo@gmail.com"
  ],
  "downtimeEnabled": "ENABLE_MAINTENANCE",
  "paths": {
    "rootdir": "../",
    "migrations": "../migrations/",
    "controllers": "../src/app/controllers/",
    "serializers": "../src/app/serializers",
    "models": "../src/app/models",
    "schema": "../src/db/schema",
    "logs": "../logs",
    "tests": "../test"
  },
  "security": {
    "secret": "SECRET"
  },
  "api": {
    "google": {
      "geoLocation": {
        "apiKey": "GOOGLE_GEOLOCATION_API_KEY"
      }
    },
    "paystack": {
      "baseUrl": "https://api.paystack.co",
      "publicKey": "PAYSTACK_PUBLIC_KEY",
      "secretKey": "PAYSTACK_SECRET_KEY"
    },
    "flutterwave": {
      "baseUrl": "https://api.ravepay.co",
      "publicKey": "RAVEPAY_PUBLIC_KEY",
      "secretKey": "RAVEPAY_SECRET_KEY",
      "webhookHash": "RAVEPAY_VERIFY_HASH"
    },
    "stripe": {
      "baseUrl": "https://api.stripe.com",
      "secretKey": "STRIPE_SECRET_KEY",
      "publicKey": "STRIPE_PUBLIC_KEY"
    }
  },
  "modules": {
    "onesignal": {
      "appId": "ONESIGNAL_APPID",
      "apiKey": "ONESIGNAL_APIKEY"
    },
    "bulksms": {
      "apiKey": "BULKSMS_APIKEY",
      "senderId": "Rise Vest"
    },
    "firebase": {
      "apiKey": "FIREBASE_API_KEY",
      "projectId": "FIREBASE_PROJECT_ID"
    },
    "sequelize": {
      "url": "DATABASE_URL",
      "logging": "DB_LOGGING"
    },
    "auth": {
      "jwtTTL": 31536000
    }
  },
  "aws": {
    "accessKey": "AWS_KEY",
    "secret": "AWS_SECRET",
    "s3": {
      "region": "us-east-1",
      "bucket": {
        "name": "atcapp"
      }
    },
    "ses": {
      "sender": "SENDER_EMAIL",
      "domain": "AWS_SES_DOMAIN"
    }
  },
  "featureFlags": {
    "stripeTest": "STRIPE_TEST",
    "enableNotifier": "1",
    "defaultProvider": "flutterwave"
  },
  "settings": {}
}
