{
  "name": "atc-backend-base",
  "version": "1.0.0",
  "description": "ATC API (Docker, NodeJs, ExpressJs, Sequelize)",
  "main": "./server/app.js",
  "scripts": {
    "createdb": "./node_modules/.bin/sequelize db:create",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "rollback": "./node_modules/.bin/sequelize db:migrate:undo",
    "rollback:all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "start": "node --experimental-modules server/app.js",
    "dev": "npm run createdb && npm run migrate && nodemon server/app.js",
    "prod": "npm run createdb && npm run migrate && node server/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hackninjas/atc-backend-base.git"
  },
  "author": "Afeez Awotundun",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "blueimp-md5": "^2.10.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.22.0",
    "cors": "^2.8.5",
    "csv-express": "^1.2.2",
    "datauri": "^3.0.0",
    "dotenv": "^8.0.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "fetch": "^1.1.0",
    "fs": "0.0.1-security",
    "geolib": "^3.0.4",
    "handlebars": "^4.7.6",
    "hashids": "^1.2.2",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongodb": "^3.3.4",
    "mongoose": "^5.9.26",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^2.1.0",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.2.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "pug": "^3.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sequelize": "^6.3.3",
    "sequelize-cli": "^6.2.0",
    "stripe": "^8.78.0",
    "uuid": "^3.3.2"
  },
