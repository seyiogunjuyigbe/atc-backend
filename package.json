  "license": "MIT",
  "main": "src/app.js",
  "scripts": {
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "setupdbs": "bin/bms createdb -d && bin/bms createdb -t",
    "rollback": "./node_modules/.bin/sequelize db:migrate:undo",
    "rollback:all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "audit": "npm-audit",
    "test": "NODE_ENV=test npm run migrate && DB_LOGGING=false LOG_ERRORS=false mocha --exit --recursive --reporter dot \"test/**/*.spec.ts\"",
    "test:verbose": "NODE_ENV=test npm run migrate && DB_LOGGING=true LOG_ERRORS=true mocha --exit --recursive --reporter dot \"test/**/*.spec.ts\"",
    "serve": "npm run migrate && node src/app.js",
    "coverage": "nyc --include=\"src/**/*.ts\" --reporter=text --reporter=html --reporter=lcov mocha --exit --recursive \"test/**/*.spec.ts\"",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "dev": "nodemon --exec \"node --require dotenv/config\" --watch .",
    "prod": "npm run migrate && node src/app.js",
    "stag": "node src/app.js"
  },
